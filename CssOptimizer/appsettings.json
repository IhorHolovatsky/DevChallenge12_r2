{
    "Logging": {
        "IncludeScopes": false,
        "Debug": {
            "LogLevel": {
                "Default": "Warning"
            }
        },
        "Console": {
            "LogLevel": {
                "Default": "Warning"
            }
        }
    },

    "ChromeSessionPool": {
        "ChromeDebuggingPort": 9222,
        "IsHeadlessMode": true,

        "MaxSessionPoolCount": 10,
        //In seconds
        "CommandTimeout": 120,
        "RequestTimeout": 120
    },

    "Cache": {
        //in seconds
        "UrlCacheTime": 3600
    } 
}
